<h1>Articles</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Is Published</th>
    <th>Total Words</th>
    <th>Author</th>
    <th> Created At </th>
    <th> Updated At </th>
  </tr>

  <% articles.forEach(function(article) { %>  

    <tr>
    <td> <%= article.title %> </td>
    <td> <%= article.content %> </td>
    <td> <%= article.isPublished %> </td>
    <td> <%= article.totalWords %> </td>
    <td> <%= article.author %> </td>
    <td> <%= moment(article.createdAt).fromNow() %> </td>
    <td> <%= moment(article.updatedAt).fromNow() %> </td>
    </tr>

  <% }) %>

</table>